<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/CsLayout_ep"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/splash" >

    <ImageView
        android:id="@+id/iv_dophin_ep"
        android:layout_width="127dp"
        android:layout_height="122dp"
        android:layout_marginTop="28dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.464"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/dolphin2" />

    <TextView
        android:id="@+id/tv_name_ep"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:layout_marginBottom="8dp"
        android:text="Name"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/iv_name_ep"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/iv_name_ep"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.317"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_dophin_ep"
        app:srcCompat="@drawable/long_rect_pp" />

    <ImageView
        android:id="@+id/iv_username_ep"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.317"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_name_ep"
        app:srcCompat="@drawable/long_rect_pp" />

    <TextView
        android:id="@+id/tv_number_ep"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:layout_marginBottom="12dp"
        android:text="Phone Number"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/iv_num_ep"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/tv_birthday_ep"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:layout_marginBottom="8dp"
        android:text="Birthday"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/iv_birthday_ep"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/tv_email_ep"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:layout_marginBottom="8dp"
        android:text="Email"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/iv_email_ep"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/tv_username_ep"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:layout_marginBottom="8dp"
        android:text="Username"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/iv_username_ep"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/iv_birthday_ep"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:layout_marginTop="44dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_username_ep"
        app:srcCompat="@drawable/long_rect_pp" />

    <ImageView
        android:id="@+id/iv_email_ep"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:layout_marginTop="44dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_birthday_ep"
        app:srcCompat="@drawable/long_rect_pp" />

    <Button
        android:id="@+id/blt_dlt_ep"
        android:layout_width="211dp"
        android:layout_height="48dp"
        android:layout_marginStart="88dp"
        android:layout_marginTop="33dp"
        android:layout_marginEnd="112dp"
        android:text="Delete Account"
        android:textColor="#AFBA0404"
        android:textColorLink="#B6BAB4B4"
        android:background="@drawable/long_rect_pp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_num_ep" />

    <EditText
        android:id="@+id/et_name_ep"
        android:layout_width="243dp"
        android:layout_height="48dp"
        android:layout_marginTop="48dp"
        android:ems="10"
        android:inputType="text"
        android:hint="name"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.351"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_dophin_ep" />

    <EditText
        android:id="@+id/et_username_ep"
        android:layout_width="245dp"
        android:layout_height="48dp"
        android:ems="10"
        android:inputType="text"
        android:hint="username"
        app:layout_constraintBottom_toBottomOf="@+id/iv_username_ep"
        app:layout_constraintEnd_toEndOf="@+id/iv_username_ep"
        app:layout_constraintHorizontal_bias="0.555"
        app:layout_constraintStart_toStartOf="@+id/iv_username_ep"
        app:layout_constraintTop_toTopOf="@+id/iv_username_ep"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/et_birthday_ep"
        android:layout_width="239dp"
        android:layout_height="48dp"
        android:ems="10"
        android:inputType="text"
        android:hint="birthday"
        app:layout_constraintBottom_toBottomOf="@+id/iv_birthday_ep"
        app:layout_constraintEnd_toEndOf="@+id/iv_birthday_ep"
        app:layout_constraintHorizontal_bias="0.375"
        app:layout_constraintStart_toStartOf="@+id/iv_birthday_ep"
        app:layout_constraintTop_toTopOf="@+id/iv_birthday_ep" />

    <EditText
        android:id="@+id/et_email_ep"
        android:layout_width="239dp"
        android:layout_height="48dp"
        android:layout_marginBottom="8dp"
        android:ems="10"
        android:inputType="text"
        android:hint="email"
        app:layout_constraintBottom_toBottomOf="@+id/iv_email_ep"
        app:layout_constraintEnd_toEndOf="@+id/iv_email_ep"
        app:layout_constraintHorizontal_bias="0.375"
        app:layout_constraintStart_toStartOf="@+id/iv_email_ep" />

    <EditText
        android:id="@+id/et_num_ep"
        android:layout_width="236dp"
        android:layout_height="48dp"
        android:ems="10"
        android:inputType="text"
        android:hint="number"
        app:layout_constraintBottom_toBottomOf="@+id/iv_num_ep"
        app:layout_constraintEnd_toEndOf="@+id/iv_num_ep"
        app:layout_constraintStart_toStartOf="@+id/iv_num_ep"
        app:layout_constraintTop_toTopOf="@+id/iv_num_ep"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/iv_num_ep"
        android:layout_width="260dp"
        android:layout_height="57dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="60dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_email_ep"
        app:srcCompat="@drawable/long_rect_pp" />

    <Button
        android:id="@+id/btn_back_ep"
        android:layout_width="86dp"
        android:layout_height="48dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/short_rect_pp"
        android:text="back"
        android:textColor="#4B4444"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>