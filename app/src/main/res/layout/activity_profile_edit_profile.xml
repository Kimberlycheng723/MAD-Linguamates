<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/CsLayout_ep"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/splash">

    <ImageView
        android:id="@+id/iv_dophin_ep"
        android:layout_width="127dp"
        android:layout_height="122dp"
        android:layout_marginTop="32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.464"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/dolphin2" />

    <TextView
        android:id="@+id/tv_name_ep"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="52dp"
        android:layout_marginTop="109dp"
        android:layout_marginBottom="4dp"
        android:fontFamily="@font/poppins_semibold"
        android:text="Name"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/iv_name_ep"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_back_ep" />

    <ImageView
        android:id="@+id/iv_username_ep"
        android:layout_width="322dp"
        android:layout_height="59dp"
        android:layout_marginTop="32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_name_ep"
        app:srcCompat="@drawable/long_rect_pp" />

    <ImageView
        android:id="@+id/iv_name_ep"
        android:layout_width="322dp"
        android:layout_height="69dp"
        android:layout_marginTop="48dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_dophin_ep"
        app:srcCompat="@drawable/long_rect_pp" />

    <EditText
        android:id="@+id/et_name_ep"
        android:layout_width="247dp"
        android:layout_height="48dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="52dp"
        android:background="@null"
        android:ems="10"
        android:fontFamily="@font/poppins_semibold"
        android:hint="name"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.396"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_dophin_ep" />

    <EditText
        android:id="@+id/et_username_ep"
        android:layout_width="295dp"
        android:layout_height="48dp"
        android:layout_marginStart="10dp"
        android:background="@null"
        android:ems="10"
        android:fontFamily="@font/poppins_semibold"
        android:hint="username"
        android:inputType="text"
        app:layout_constraintBottom_toBottomOf="@+id/iv_username_ep"
        app:layout_constraintEnd_toEndOf="@+id/iv_username_ep"
        app:layout_constraintStart_toStartOf="@+id/iv_username_ep"
        app:layout_constraintTop_toTopOf="@+id/iv_username_ep"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/tv_username_ep"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="52dp"
        android:layout_marginBottom="4dp"
        android:fontFamily="@font/poppins_semibold"
        android:text="Username"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/iv_username_ep"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/et_num_ep"
        android:layout_width="295dp"
        android:layout_height="61dp"
        android:background="@null"
        android:ems="10"
        android:fontFamily="@font/poppins_semibold"
        android:hint="number"
        android:inputType="text"
        app:layout_constraintBottom_toBottomOf="@+id/iv_num_ep"
        app:layout_constraintEnd_toEndOf="@+id/iv_num_ep"
        app:layout_constraintHorizontal_bias="0.814"
        app:layout_constraintStart_toStartOf="@+id/iv_num_ep"
        app:layout_constraintTop_toTopOf="@+id/iv_num_ep"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/tv_number_ep"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="52dp"
        android:layout_marginBottom="4dp"
        android:fontFamily="@font/poppins_semibold"
        android:text="Phone Number"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/iv_num_ep"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/iv_num_ep"
        android:layout_width="322dp"
        android:layout_height="72dp"
        android:layout_marginTop="32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_email_ep"
        app:srcCompat="@drawable/long_rect_pp" />

    <TextView
        android:id="@+id/tv_email_ep"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="52dp"
        android:layout_marginBottom="8dp"
        android:fontFamily="@font/poppins_semibold"
        android:text="Email"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/iv_email_ep"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/et_email_ep"
        android:layout_width="271dp"
        android:layout_height="48dp"
        android:background="@null"
        android:ems="10"
        android:fontFamily="@font/poppins_semibold"
        android:hint="email"
        android:inputType="text"
        app:layout_constraintBottom_toBottomOf="@+id/iv_email_ep"
        app:layout_constraintEnd_toEndOf="@+id/iv_email_ep"
        app:layout_constraintStart_toStartOf="@+id/iv_email_ep"
        app:layout_constraintTop_toTopOf="@+id/iv_email_ep" />

    <ImageView
        android:id="@+id/iv_email_ep"
        android:layout_width="322dp"
        android:layout_height="58dp"
        android:layout_marginTop="36dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_username_ep"
        app:srcCompat="@drawable/long_rect_pp" />

    <Button
        android:id="@+id/btn_back_ep"
        android:layout_width="86dp"
        android:layout_height="48dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/short_rect_pp"
        android:fontFamily="@font/poppins_semibold"
        android:text="back"
        android:textColor="#4B4444"
        style="@style/CustomUnitButton"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_save_ep"
        style="@style/CustomUnitButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="32dp"
        android:background="@drawable/short_rect_pp"
        android:fontFamily="@font/poppins_semibold"
        android:text="Save"
        android:textColor="#4B4444"
        app:layout_constraintBottom_toTopOf="@+id/blt_dlt_ep"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.461"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/blt_dlt_ep"
        style="@style/CustomUnitButton"
        android:layout_width="211dp"
        android:layout_height="48dp"
        android:layout_marginStart="88dp"
        android:layout_marginEnd="112dp"
        android:layout_marginBottom="44dp"
        android:background="@drawable/long_rect_pp"
        android:fontFamily="@font/poppins_semibold"
        android:text="Delete Account"
        android:textColor="#AFBA0404"
        android:textColorLink="#B6BAB4B4"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/iv_bubble_ep"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="17dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="6dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.225"
        app:layout_constraintStart_toEndOf="@+id/iv_dophin_ep"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/bubble" />

    <ImageView
        android:id="@+id/iv_bubble2_ep"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="79dp"
        android:layout_marginEnd="125dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/iv_dophin_ep"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/bubble" />

    <ImageView
        android:id="@+id/iv_bubble3_ep"
        android:layout_width="37dp"
        android:layout_height="43dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="7dp"
        app:layout_constraintEnd_toStartOf="@+id/iv_dophin_ep"
        app:layout_constraintStart_toEndOf="@+id/btn_back_ep"
        app:layout_constraintTop_toBottomOf="@+id/btn_back_ep"
        app:srcCompat="@drawable/bubble" />

    <TextView
        android:id="@+id/tv_bio_ep"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="52dp"
        android:fontFamily="@font/poppins_semibold"
        android:text="Bio"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/imageView5"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/et_bio_ep"
        android:layout_width="291dp"
        android:layout_height="54dp"
        android:background="@android:color/transparent"
        android:ems="10"
        android:fontFamily="@font/poppins_semibold"
        android:hint="this is my bio and my bio is me "
        android:inputType="text"
        app:layout_constraintBottom_toBottomOf="@+id/imageView5"
        app:layout_constraintEnd_toEndOf="@+id/imageView5"
        app:layout_constraintStart_toStartOf="@+id/imageView5"
        app:layout_constraintTop_toTopOf="@+id/imageView5" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="322dp"
        android:layout_height="73dp"
        android:layout_marginTop="36dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/et_num_ep"
        app:srcCompat="@drawable/long_rect_pp" />

</androidx.constraintlayout.widget.ConstraintLayout>